<core:FragmentDefinition controllerName="monitorpedidos.controller.AltaPedidos"
    xmlns:m="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:l="sap.ui.layout"
    xmlns:table="sap.ui.table">
    <!--    -->
    <m:VBox renderType="Bare">
        <table:Table selectionMode="None" id="TablaPosiciones" rows="{path: ''}">
            <table:Column hAlign="Center">
                <m:Label text="{i18n>Material}"/>
                <table:template>
                    <m:Text text=""/>
                </table:template>
            </table:Column>
            <table:Column hAlign="Center">
                <m:Label text="{i18n>Descripcion}" />
                <table:template>
                    <m:Text text=""/>
                </table:template>
            </table:Column>
            <table:Column hAlign="Center">
                <m:Label text="{i18n>Cantidad}" />
                <table:template>
                    <m:Text text=""/>
                </table:template>
            </table:Column>
            <table:Column hAlign="Center">
                <m:Label text="{i18n>Unidades}" />
                <table:template>
                    <m:Text text=""/>
                </table:template>
            </table:Column>
            <table:Column hAlign="Center">
                <m:Label text="{i18n>Importe}" />
                <table:template>
                    <m:Text text=""/>
                </table:template>
            </table:Column>
            <table:Column hAlign="Center">
                <m:Label text="{i18n>Moneda}" />
                <table:template>
                    <m:Text text=""/>
                </table:template>
            </table:Column>
        </table:Table>
    </m:VBox>

</core:FragmentDefinition>